{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "FiraCode Nerd Font Mono Ret",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono Ret",
    "git.openRepositoryInParentFolders": "never",
    "workbench.settings.applyToAllProfiles": [
        "editor.fontFamily"
    ],
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "editor.guides.indentation": false,
    "editor.stickyScroll.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 3,
    "editor.scrollbar.horizontalScrollbarSize": 3,
    "editor.lightbulb.enabled": "off",
    "editor.overviewRulerBorder": false,
    "editor.glyphMargin": false,
    "editor.renderLineHighlight": "all",
    "editor.folding": true,
    "workbench.editor.showIcons": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editor.tabSizing": "shrink",
    "workbench.layoutControl.enabled": true,
    "workbench.layoutControl.type": "menu",
    "workbench.sideBar.location": "right",
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.tabs.enabled": true,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "none",
    "telemetry.telemetryLevel": "off",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.emptySelectionClipboard": false,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "trailing",
    "editor.rulers": [ 80, { "column": 100, "color": "#997777" } ],
    "editor.unicodeHighlight.allowedCharacters": { "Ñƒ": true },
    "window.title": " ",
    "window.titleBarStyle": "native",
    "window.menuBarVisibility": "hidden",
    "files.enableTrash": false,
    "files.exclude": {
        ".cache-loader/": true,
        ".gitattributes": true,
        ".gitkeep": true,
        ".travis.yml": true,
        ".vercel": true,
        ".vscode/": true,
        "coverage": true,
        "acf-json": true,
        "backend/vendor": true,
        "CODE_OF_CONDUCT.md": true,
        "config": false,
        "LICENSE.md": true,
        "package-lock.json": true,
        "phpcs.xml": true,
    },
    "workbench.colorTheme": "Monokai++ Unified",
    "git.confirmSync": false,
    "window.zoomPerWindow": false,
    "window.commandCenter": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.enablePreview": false,
    "window.zoomLevel": 2
}
