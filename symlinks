# User-global git configuration
~/.gitconfig -> gitconfig

# Awesome wm config
~/.config/awesome -> awesome

~/.local/share/applications/browsers.desktop -> execs/browsers.desktop
~/.local/share/applications/launcher.desktop -> execs/launcher.desktop
~/.gnupg/gpg.conf -> gpg.conf
~/.gnupg/gpg-agent.conf -> gpg-agent.conf

~/.config/nnn/plugins -> nnn-plugins
~/.config/kitty/kitty.conf -> kitty.conf
~/.config/nix/nix.conf -> nix.conf

~/.config/rofi/config.rasi -> rofi.rasi

~/.xinitrc -> xinitrc
~/.config/git/ignore -> gitignore

